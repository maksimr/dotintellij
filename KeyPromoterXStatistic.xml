          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌃F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="YouTrack">
        <Item>
          <IdeaActionID>ActivateYouTrackToolWindow</IdeaActionID>
          <ShortCut>'⌥,, Y'</ShortCut>
          <Description>YouTrack</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>