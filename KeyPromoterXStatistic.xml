<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Activate Armory">
        <Item>
          <IdeaActionID>armory.activateArmory</IdeaActionID>
          <ShortCut>'⌃⏎' or '⌧' or 'Begin'</ShortCut>
          <Description>Activate Armory</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>armory.box.DeleteTree</IdeaActionID>
          <ShortCut>'⌃W'</ShortCut>
          <Description>Close</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌃C'</ShortCut>
          <Description>Copy</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌃⌥⌫'</ShortCut>
          <Description>Delete</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Highlight current scope">
        <Item>
          <IdeaActionID>Rainbow.ScopeHighlightingAction</IdeaActionID>
          <ShortCut>'⌃Button3 Click'</ShortCut>
          <Description>Highlight current scope</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Navigation Bar">
        <Item>
          <IdeaActionID>ShowNavBar</IdeaActionID>
          <ShortCut>'⌥↖' or '⌥,, N'</ShortCut>
          <Description>Jump to Navigation Bar</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Type Source">
        <Item>
          <IdeaActionID>Flutter.JumpToTypeSource</IdeaActionID>
          <ShortCut>'⇧F4'</ShortCut>
          <Description>Jump to Type Source</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'Wallaby'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'⌃F5' or '⌥,, ⇧R'</ShortCut>
          <Description>Rerun 'Wallaby'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'e2e'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
