          <Description>Rerun 'e2e'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'main.dart'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'main.dart'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Shelve Changes...">
        <Item>
          <IdeaActionID>ChangesView.Shelve</IdeaActionID>
          <ShortCut>'⌥,, S'</ShortCut>
          <Description>Shelve Changes...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Tree Menu">
        <Item>
          <IdeaActionID>armory.treeHdr.showMenu</IdeaActionID>
          <ShortCut>'⌥Context Menu'</ShortCut>
          <Description>Show Tree Menu</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'All Tests (wallaby)'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'All Tests (wallaby)'</Description>
          <Count>1</Count>
