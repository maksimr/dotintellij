          <Description>Stop 'Unit Tests'</Description>
          <Count>10</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Wallaby'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'Wallaby'</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'e2e'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'e2e'</Description>
