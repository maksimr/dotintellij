<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Close">
        <Item>
          <IdeaActionID>armory.box.DeleteTree</IdeaActionID>
          <ShortCut>'⌃W'</ShortCut>
          <Description>Close</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌃C'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'main.dart'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⇧F10'</ShortCut>
          <Description>Run 'main.dart'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Tree Menu">
        <Item>
          <IdeaActionID>armory.treeHdr.showMenu</IdeaActionID>
          <ShortCut>'⌥Context Menu'</ShortCut>
          <Description>Show Tree Menu</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'main.dart'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌃F2'</ShortCut>
          <Description>Stop 'main.dart'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌃F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="YouTrack">
        <Item>
          <IdeaActionID>ActivateYouTrackToolWindow</IdeaActionID>
          <ShortCut>'⌥,, Y'</ShortCut>
          <Description>YouTrack</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>