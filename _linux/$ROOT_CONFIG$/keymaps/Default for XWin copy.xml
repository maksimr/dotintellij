    <keyboard-shortcut first-keystroke="control F5" />
    <keyboard-shortcut first-keystroke="shift alt R" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="shift alt MINUS" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="shift alt BACK_SLASH" />
  </action>
  <action id="SwitchApply">
    <keyboard-shortcut first-keystroke="shift alt ENTER" />
  </action>
  <action id="SwitchDown">
    <keyboard-shortcut first-keystroke="shift alt DOWN" />
  </action>
  <action id="SwitchLeft">
    <keyboard-shortcut first-keystroke="shift alt LEFT" />
  </action>
  <action id="SwitchRight">
    <keyboard-shortcut first-keystroke="shift alt RIGHT" />
  </action>
  <action id="SwitchUp">
    <keyboard-shortcut first-keystroke="shift alt UP" />
  </action>
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="shift alt U" />
  </action>
  <action id="com.wallaby.javascript.actions.ShowLineTestsAction">
    <keyboard-shortcut first-keystroke="alt COMMA" second-keystroke="I" />
  </action>
  <action id="org.intellij.ideaplugins.tabswitch.action.NextTabAction">
    <keyboard-shortcut first-keystroke="alt A" />
  </action>
  <action id="org.jetbrains.plugins.terminal.OpenLocalTerminalAction">
    <keyboard-shortcut first-keystroke="shift alt PERIOD" />
  </action>
